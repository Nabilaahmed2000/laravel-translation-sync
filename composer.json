{
    "name": "nabila/laravel-translation-sync",
    "description": "A Laravel package to detect untranslated strings and automatically translate them using various translation services like Google Translate, DeepL, and more.",
    "type": "library",
    "keywords": [
        "laravel",
        "translation",
        "localization",
        "i18n",
        "google-translate",
        "deepl",
        "automation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nabila",
            "email": "nabila@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nabila\\TranslationSync\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "stichoza/google-translate-php": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nabila\\TranslationSync\\TranslationSyncServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
