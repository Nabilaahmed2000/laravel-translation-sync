{
    "name": "nabila/laravel-translation-sync",
    "description": "A Laravel package to detect untranslated strings and automatically translate them using various translation services like Google Translate, DeepL, and more.",
    "version": "2.1.0",
    "type": "library",
    "keywords": [
        "laravel",
        "translation",
        "localization",
        "i18n",
        "google-translate",
        "deepl",
        "automation"
    ],
    "homepage": "https://github.com/Nabilaahmed2000/laravel-translation-sync",
    "support": {
        "issues": "https://github.com/Nabilaahmed2000/laravel-translation-sync/issues",
        "source": "https://github.com/Nabilaahmed2000/laravel-translation-sync"
    },
    "autoload": {
        "psr-4": {
            "Nabila\\TranslationSync\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "stichoza/google-translate-php": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nabila\\TranslationSync\\TranslationSyncServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
